# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Hydrator' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Hydrator
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'GoogleSignIn'
pod 'FBSDKCoreKit'
pod 'FBSDKShareKit'
pod 'FBSDKLoginKit'
pod 'TwitterKit'
pod 'SVProgressHUD'
pod 'CircleProgressView'
pod 'SwiftDate'

end
